{
  "name": "@eatonfyi/ljarchive",
  "version": "0.1.1",
  "description": "A JS parser for LJArchive files",
  "engines": {
    "node": "^20.10.0"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "run": "node --import=tsimp src/index.ts",
    "demo": "node --import=tsimp src/test.ts",
    "build": "rm -rf dist; rm -rf .tsimp; pkgroll --minify",
    "format": "prettier --config prettier.config.js 'src/**/*.ts' --write",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eaton/ljarchive"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "ava": "^6.1.2",
    "pkgroll": "^2.0.2",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "tsimp": "^2.0.11",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@sindresorhus/is": "^7.0.0",
    "@thi.ng/leb128": "^3.1.6",
    "dayjs": "^1.11.11",
    "fast-xml-parser": "^4.4.0",
    "fs-jetpack": "^5.1.0",
    "zod": "^3.23.8"
  }
}
